{
    "name": "zendframework/zend-hydrator",
    "description": " ",
    "license": "BSD-3-Clause",
    "keywords": [
        "zf2",
        "hydrator"
    ],
    "homepage": "https://github.com/zendframework/zend-hydrator",
    "autoload": {
        "psr-4": {
            "Zend\\Hydrator\\": "src/"
        }
    },
    "require": {
        "php": "^5.6 || ^7.0",
        "zendframework/zend-stdlib": "^3.0"
    },
    "require-dev": {
        "zendframework/zend-eventmanager": "^3.0",
        "zendframework/zend-inputfilter": "^2.6",
        "zendframework/zend-serializer": "^2.6.1",
        "zendframework/zend-servicemanager": "^3.0.3 || ^2.7.8",
        "zendframework/zend-filter": "^2.6",
        "phpunit/phpunit": "^6.0.8 || ^5.7.15",
        "squizlabs/php_codesniffer": "^2.3.1"
    },
    "suggest": {
        "zendframework/zend-eventmanager": "^3.0 || ^2.6.2, to support aggregate hydrator usage",
        "zendframework/zend-serializer": "^2.6.1, to use the SerializableStrategy",
        "zendframework/zend-servicemanager": "^3.0.3 || ^2.7.5, to support hydrator plugin manager usage",
        "zendframework/zend-filter": "^2.6, to support naming strategy hydrator usage"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-release-1.0": "1.0-dev",
            "dev-release-1.1": "1.1-dev",
            "dev-master": "2.2-dev",
            "dev-develop": "2.3-dev"
        },
        "zf": {
            "component": "Zend\\Hydrator",
            "config-provider": "Zend\\Hydrator\\ConfigProvider"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ZendTest\\Hydrator\\": "test/"
        }
    }
}
